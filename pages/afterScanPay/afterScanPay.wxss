.container {
  background: #f0f0f0;
}
.success {
  width: 100%;
  height: 100;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  padding-bottom: 40rpx;
}
.success .icon {
  margin-top: 101rpx;
}
.success .title {
  font-size: 40rpx;
  margin-top: 60rpx;
  margin-bottom: 64rpx;
}
.success .order-no-title {
  color: #999;
  font-size: 30rpx;
  margin-top: 62rpx;
}
.success .order-no-con {
  margin-top: 28rpx;
  font-size: 26rpx;
  color: #999;
  margin-bottom: 28rpx;
}
.success .order-no-con .onder-no-one {
  line-height: 36 rpx;
}
.success .more {
  height: 52rpx;
  line-height: 52rpx;
  border-radius: 10rpx;
  border: 1rpx solid #999;
  font-size: 26rpx;
  color: #999;
  padding: 0 26rpx;
}
.success .more .array {
  display: inline-block;
  width: 14rpx;
  height: 14rpx;
  border-left: 4rpx solid #999;
  border-bottom: 4rpx solid #999;
  transform: rotateZ(-45deg) translateY(-5rpx);
  margin-right: 16rpx;
}
.success .more .rotate {
  transform: rotateZ(135deg);
}
.success .more .num {
  display: inline-block;
  line-height: 52rpx;
}
.success .detail-one {
  width: 100%;
  padding-left: 196rpx;
  display: flex;
  align-items: flex-start;
  justify-content: flex-start;
  font-size: 28rpx;
  color: #999;
}
.success .detail-one .left {
  line-height: 56rpx;
  width: 124rpx;
  text-align: left;
}
.success .detail-one .right {
  line-height: 56rpx;
}
.success .detail-one .discount-one {
  line-height: 56rpx;
}
.success button {
  width: 360rpx;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 32rpx;
  margin-top: 64rpx;
}
.success .show-order {
  background: #fbfafc;
  color: #000;
  border: 1rpx solid #d6d5d6;
}
