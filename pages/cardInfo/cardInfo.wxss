.container{
    width:100%;
    height:100vh;
    background: #f0f0f0;
    border-top:1px solid #dbdbdb;
}
.all{
    display: flex;
    justify-content: space-between;
    align-items: center;
    height:87rpx;
    padding:0 41rpx;
    background:#fff;
    border-bottom:1px solid #dbdbdb;
    font-size: 34rpx;
}
.all>view:nth-child(2){
    color:#333;
}
.all>view:last-child{
    display: flex;
    justify-content: right;
    align-items: center;
    color:#333;
}
.all>view:last-child view{
    margin-left:28rpx;
}
.getMore{
  height:87rpx;
  text-align: center;
  line-height: 87rpx;
  font-size: 24rpx;
  color:#999;
}
.noOrder{
  display: flex;
  flex-direction: column;
  justify-content: left;
  align-items: center;
}
.noOrder image{
  margin-top:100rpx;
  width:320rpx;
  height:320rpx;
}
.noOrder view{
  margin-top:36rpx;
  font-size: 34rpx;
  color:#999;
}
