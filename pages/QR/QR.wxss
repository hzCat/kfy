.container .vip,
.container .tvip {
  width: 100%;
  height: 100%;
  padding-top: 104rpx;
}
.container .vip .con,
.container .tvip .con {
  width: 650rpx;
  height: 796rpx;
  margin-left: 50rpx;
  background: #fff;
  border-radius: 8rpx;
  position: relative;
  overflow: hidden;
}
.container .vip {
  background: #efac40;
}
.container .tvip {
  background: #d6524a;
}
.container .not-vip .info,
.container .not-tvip .info {
  height: 676rpx;
}
.container .not-vip .info image,
.container .not-tvip .info image {
  width: 100%;
  height: 100%;
}
.container .not-vip .bottom,
.container .not-tvip .bottom {
  height: 120rpx;
  border: 0;
  border-radius: 0;
  position: absolute;
  bottom: 0;
  line-height: 120rpx;
  width: 100%;
  background: #89bf21;
  color: #fff;
}
.container .not-vip .bottom::after,
.container .not-tvip .bottom::after {
  border: 0;
  border-radius: 0;
}
.container .not-vip .bottom-t,
.container .not-tvip .bottom-t {
  background: #bad839;
}
.container .is-vip,
.container .is-tvip {
  position: relative;
  text-align: center;
}
.container .is-vip .title,
.container .is-tvip .title {
  font-size: 34rpx;
  font-weight: bold;
  padding-top: 70rpx;
}
.container .is-vip .QR-con,
.container .is-tvip .QR-con {
  position: relative;
  margin-top: 24rpx;
  height: 516rpx;
}
.container .is-vip .QR-con .QR,
.container .is-tvip .QR-con .QR {
  width: 400rpx;
  height: 400rpx;
}
.container .is-vip .QR-con .get-QR,
.container .is-tvip .QR-con .get-QR {
  position: absolute;
  top: 104rpx;
  left: 213rpx;
}
.container .is-vip .QR-con .get-QR image,
.container .is-tvip .QR-con .get-QR image {
  height: 120rpx;
  width: 94rpx;
}
.container .is-vip .QR-con .get-QR view,
.container .is-tvip .QR-con .get-QR view {
  font-size: 34rpx;
  margin-top: 42rpx;
}
.container .is-vip .vip-show,
.container .is-tvip .vip-show {
  padding-top: 16rpx;
}
.container .is-vip .vip-show .info,
.container .is-tvip .vip-show .info {
  color: #999;
  font-size: 30rpx;
  text-align: center;
  line-height: 34rpx;
  display: block;
  letter-spacing: 0.4em;
}
.container .is-vip .vip-show .money,
.container .is-tvip .vip-show .money {
  margin-top: 16rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 34rpx;
  line-height: 34rpx;
}
.container .is-vip .vip-show .money .balance,
.container .is-tvip .vip-show .money .balance {
  color: #faaf31;
}
.container .is-vip .vip-show .money .charge,
.container .is-tvip .vip-show .money .charge {
  padding-left: 26rpx;
  color: #12bfff;
}
.container .is-vip .vip-show .money .dealing,
.container .is-tvip .vip-show .money .dealing {
  color: #87d463;
}
.container .is-vip .vip-show .money .strong,
.container .is-tvip .vip-show .money .strong {
  height: 34rpx;
  color: #faaf31;
  font-weight: bold;
  padding-right: 33rpx;
  border-right: 2rpx solid #ccc;
}
.container .is-vip .tips,
.container .is-tvip .tips {
  position: absolute;
  bottom: 70rpx;
  width: 100%;
  text-align: center;
  font-size: 24rpx;
  color: #999;
  line-height: 36rpx;
}
