.container .vip,
.container .tvip {
  width: 100%;
  height: 100%;
  padding-top: 104rpx;
}
.container .vip .con,
.container .tvip .con {
  width: 650rpx;
  height: 796rpx;
  margin-left: 50rpx;
  background: #fff;
  border-radius: 8rpx;
  position: relative;
  overflow: hidden;
}
.container .vip {
  background: #efac40;
}
.container .tvip {
  background: #d6524a;
}
.container .not-vip .info,
.container .not-tvip .info {
  height: 676rpx;
}
.container .not-vip .info image,
.container .not-tvip .info image {
  width: 100%;
  height: 100%;
}
.container .not-vip .bottom,
.container .not-tvip .bottom {
  height: 120rpx;
  border: 0;
  border-radius: 0;
  position: absolute;
  bottom: 0;
  line-height: 120rpx;
  width: 100%;
  background: #89bf21;
  color: #fff;
}
.container .not-vip .bottom::after,
.container .not-tvip .bottom::after {
  border: 0;
  border-radius: 0;
}
.container .not-vip .bottom-t,
.container .not-tvip .bottom-t {
