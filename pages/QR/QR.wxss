}
.container .is-vip .title {
  font-size: 34rpx;
  font-weight: bold;
  padding-top: 70rpx;
}
.container .is-vip .QR-con {
  position: relative;
  margin-top: 24rpx;
  height: 492rpx;
}
.container .is-vip .QR-con .QR {
  width: 400rpx;
  height: 400rpx;
}
.container .is-vip .QR-con .get-QR {
  position: absolute;
  top: 104rpx;
  left: 213rpx;
}
.container .is-vip .QR-con .get-QR image {
  height: 120rpx;
  width: 94rpx;
}
.container .is-vip .QR-con .get-QR view {
  font-size: 34rpx;
  margin-top: 42rpx;
}
.container .is-vip .tips {
  font-size: 24rpx;
